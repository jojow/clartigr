    "debug": "~2.0.0",
    "nedb": "~0.11",
    "uuid": "~2.0.0",
    "lodash": "~2.4.0",
    "recursive-readdir": "~1.2.0",
    "any2api-util": "*" 
  },
  "devDependencies": {
    "mocha": "~1.21.4",
    "chai": "~1.9.1",
    "supertest": "~0.14.0",
